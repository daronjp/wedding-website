<div class="hero-unit">
  <h1 class="headline large">Households</h1>
  <p></p>
  <%= link_to 'New Household', new_admin_household_path %>
  <table>
    <tr class=alternate>
      <th>Household Name</th>
      <th>Seats Name</th>
    </tr>
    <% @household.each do |q| %>
      <tr class=alternate>
        <td><%= link_to admin_household_path(q) do %>
              <%= q.household_name %>
            <% end %></td>
        <td><%= q.seats %></td>
      </tr>
    <% end %>
  </table>
</div>